{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:all", "plugin:react/all"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true,
            "es6": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-ternary": [0],
        "no-nested-ternary": [0],
        "react/forbid-prop-types": [0],
        "react/jsx-max-props-per-line": [0],
        "object-property-newline": [0],
        "no-use-before-define": [0],
        "max-len": [0],
        "id-length": [0],
        "no-console": [0],
        "no-underscore-dangle": [0],
        "no-magic-numbers": [0],
        "padded-blocks": [0],
        "no-trailing-spaces": [0],
        "class-methods-use-this": [0],
        "react/jsx-indent": [0],
        "object-curly-spacing": [0],
        "react/jsx-filename-extension": [0],
        "sort-imports": [0],
        "sort-keys": [0],
        "new-cap": [0],
        "comma-dangle": [0],
        "react/jsx-no-literals": [0],
        "react/forbid-component-props": [0],
        "jsx-filename-extension": [0],
        "capitalized-comments": [0],
        "func-call-spacing": [0],
        "quote-props": [0],
        "sort-key": [0],
        "sort-key": [0],
        "no-extra-parens": [0],
        "one-var": [0],
        "react/jsx-no-bind": [0],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
